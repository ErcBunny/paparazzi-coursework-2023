<!DOCTYPE airframe SYSTEM "../airframe.dtd">

<!-- This is an overactuated quadplane
     * Airframe:    TUD00289
     * Autopilot:   Pixhawk 4
     * Actuators:   4x BLHELI32 ESC (PWM) + 8x SAVOX servos (PWM)
     * Datalink:    Herelink/Xbee
     * GPS:         UBlox F9P
     * RC:          SBUS Crossfire
-->

<airframe name="quadplane_plus">
  <description>Overactuated vehicle</description>

  <firmware name="rotorcraft">
    <target name="ap" board="px4fmu_5.0_chibios">
      <configure name="PERIODIC_FREQUENCY" value="500"/>
      <configure name="FLASH_MODE" value="SWD"/>

<!--      <module name="radio_control" type="ppm"/>-->

      <module name="radio_control" type="sbus"> <!-- The output type of RX, over the air it can be all kinds e.g. DSMX, FRSky-->
        <define name="RADIO_CONTROL_NB_CHANNEL" value="16"/>  <!-- Set the OpenRXSR receiver to maximum channel output of 8 9ms -->
        <configure name="SBUS_PORT" value="UART3"/>  <!--Default use UART2  on FMU4 -->
        <!-- Mode set one a three way switch -->
        <!--  Per default already GEAR if not defined  <define name="RADIO_MODE" value="RADIO_GEAR"/> --><!-- yes, already done by default if not redefined to something else-->
        <define name="RADIO_AUX1" value="5"/>
        <define name="RADIO_GEAR" value="RADIO_AUX2"/>
        <define name="RADIO_FLAP" value="RADIO_AUX3"/>
      </module>

<!--      Air speed sensor-->
      <module name="airspeed" type="ms45xx_i2c">
        <define name="USE_I2C4"/>
        <define name="MS45XX_I2C_DEV" value="i2c4"/>
      </module>

<!--      <module name="airspeed_sdp3x">-->
<!--        <define name="SDP3X_I2C_DEV" value="i2c2"/>-->
<!--        <define name="SDP3X_I2C_ADDR" value="0x50"/>-->
<!--        <define name="SDP3X_SYNC_SEND" value="TRUE"/>-->
<!--        <define name="SDP3X_PRESSURE_SCALE" value="1.0"/>-->
<!--        <define name="SDP3X_PRESSURE_OFFSET" value="0.0"/>-->
<!--        <define name="SDP3X_AIRSPEED_SCALE" value="1.6327"/>-->
<!--        <define name="SDP3X_LOWPASS_TAU" value="0.15"/>-->
<!--        <define name="USE_AIRSPEED_SDP3X" value="TRUE"/>-->
<!--      </module>-->

<!--      Module needed for the communication between the raspberry pi and the pixhawk-->
      <module name="ca_am7"/>

      <!-- Logger -->
      <module name="tlsf"/>
      <module name="pprzlog"/>
      <module name="logger" type="sd_chibios"/>
      <module name="flight_recorder"/>
      <define name="SDLOG_AUTO_FLUSH_PERIOD" value="1" unit="s"/>

         <!-- Also used if QNH needed -->
      <module name="air_data">
        <define name="CALC_AIRSPEED" value="TRUE"/>
        <define name="CALC_TAS_FACTOR" value="FALSE"/>
        <define name="CALC_AMSL_BARO" value="TRUE"/>
      </module>

      <module name="geo_mag"/>

      <define name="ADC_CURRENT_DISABLE" value="TRUE"/>

      <define name="USE_PWM9" value="TRUE"/>
      <define name="USE_PWM10" value="TRUE"/>
      <define name="USE_PWM11" value="TRUE"/>
      <define name="USE_PWM12" value="TRUE"/>
      <define name="ACTUATORS_PWM_NB" value="12"/>

      <define name="RADIO_TH_HOLD" value="RADIO_AUX1"/> <!-- Throttle hold in command laws -->
      <define name="RADIO_FMODE" value="RADIO_AUX2"/> <!-- Throttle curve select -->
      <define name="RADIO_FBW_MODE" value="RADIO_AUX3"/> <!-- Switch between AP and FBW control -->
      <define name="RADIO_KILL_SWITCH" value="RADIO_AUX1"/>

      <configure name="USE_BARO_BOARD" value="FALSE"/><!-- To use in cyberzoo and OJF -->
    </target>

    <module name="telemetry" type="transparent">

<!--      Xbee-->
<!--      <configure name="MODEM_BAUD" value="B57600"/>-->

<!--      Herelink-->
      <configure name="MODEM_BAUD" value="B115200"/>

    </module>

    <module name="actuators"     type="pwm"/>
    <module name="imu"           type="mpu6000"/>

    <!--     For OUTSIDE -->

<!--    <module name="gps" type="ublox">-->
<!--      <configure name="GPS_BAUD" value="B460800"/>&lt;!&ndash; UBX message only but rate set to ~12HZ with GPS, GLONASS, BEIDU and GALILEIO at the SAME time is lots of GNSS data&ndash;&gt;-->
<!--    </module>-->

    <!--     For OPTITRACK -->

    <module name="gps"           type="datalink">
      <define name="INS_USE_GPS_ALT" value="1"/>
      <define name="INS_VFF_R_GPS" value="0.09"/>
      <define name="AHRS_HEADING_UPDATE_GPS_MIN_SPEED" value="0"/>
    </module>

    <module name="stabilization" type="indi_simple"/>
    <module name="stabilization" type="rate_indi"/>

    <module name="ins"           type="extended" />
<!--    <module name="ins" type="ekf2"/>-->

    <module name="ahrs"          type="int_cmpl_quat">
<!--      <configure name="USE_MAGNETOMETER" value="FALSE"/>-->
<!--      <define name="AHRS_USE_GPS_HEADING" value="TRUE"/>-->
    </module>

<!--    <module name="air_data"/>-->
    <module name="send_imu_mag_current"/>
    <module name="sys_mon"/>
    <module name="actuators" type="pwm">
      <define name="SERVO_HZ" value="400"/>
    </module>

    <!-- Internal MAG -->
    <module name="mag_ist8310">
      <define name="MODULE_IST8310_UPDATE_AHRS" value="TRUE"/>
      <configure name="MAG_IST8310_I2C_DEV" value="I2C3"/>
    </module>

    <!-- External MAG on GPS -->
<!--    <module name="mag_lis3mdl">-->
<!--      <define name="MODULE_LIS3MDL_UPDATE_AHRS" value="TRUE"/>-->
<!--      <configure name="MAG_LIS3MDL_I2C_DEV" value="I2C1"/>-->
<!--      <define name="LIS3MDL_CHAN_X_SIGN" value="-"/>-->
<!--      <define name="LIS3MDL_CHAN_Y_SIGN" value="-"/>-->
<!--    </module>-->

    <module name="motor_mixing"/>
    <module name="Overactuated_mixing"/>
  </firmware>

  <include href="conf/airframes/tudelft/calibrations/tudelft_am_cal_quadplane_plus_$AC_ID.xml"/>

  <commands>
    <axis name="ROLL" failsafe_value="0"/>
    <axis name="PITCH" failsafe_value="0"/>
    <axis name="YAW" failsafe_value="0"/>
    <axis name="THRUST" failsafe_value="0"/>
    <axis name="RADIO_TH_HOLD" failsafe_value="-9600"/>
  </commands>

  <rc_commands>
    <set command="THRUST" value="@THROTTLE"/>
    <set command="ROLL" value="@YAW"/>
    <set command="PITCH" value="@PITCH/2"/>
    <set command="YAW" value="-@ROLL/4"/>
  </rc_commands>

  <section name="MIXING" prefix="MOTOR_MIXING_">
    <define name="TRIM_ROLL" value="0"/>
    <define name="TRIM_PITCH" value="0"/>
    <define name="TRIM_YAW" value="0"/>
    <define name="TYPE" value="QUAD_X"/>
    <define name="REVERSE" value="true"/>
  </section>

  <command_laws>
    <call fun="overactuated_mixing_run()"/>
    <let var="th_hold" value="LessThan(RadioControlValues(RADIO_TH_HOLD), -4800)"/>
    <set servo="MOTOR_1" value="($th_hold? -9600 : overactuated_mixing.commands[0])"/>
    <set servo="MOTOR_2" value="($th_hold? -9600 : overactuated_mixing.commands[1])"/>
    <set servo="MOTOR_3" value="($th_hold? -9600 : overactuated_mixing.commands[2])"/>
    <set servo="MOTOR_4" value="($th_hold? -9600 : overactuated_mixing.commands[3])"/>
    <set servo="EL_1" value="(overactuated_mixing.commands[4])"/>
    <set servo="EL_2" value="(overactuated_mixing.commands[5])"/>
    <set servo="EL_3" value="(overactuated_mixing.commands[6])"/>
    <set servo="EL_4" value="(overactuated_mixing.commands[7])"/>
    <set servo="AZ_1" value="(overactuated_mixing.commands[8])"/>
    <set servo="AZ_2" value="(overactuated_mixing.commands[9])"/>
    <set servo="AZ_3" value="(-overactuated_mixing.commands[10])"/>
    <set servo="AZ_4" value="(-overactuated_mixing.commands[11])"/>
  </command_laws>

  <section name="MANUAL_CONTROL_OPTIONS" prefix="MANUAL_CONTROL_">
    <!-- Maximum limits of the sticks -->
    <define name="MAX_CMD_FWD_SPEED" value="20.0" unit="m/s"/>
    <define name="MAX_CMD_LAT_SPEED" value="5.0" unit="m/s"/>
    <define name="MAX_CMD_VERT_SPEED" value="2.0" unit="m/s"/>
    <define name="MAX_CMD_PITCH_ANGLE" value="25.0" unit="deg"/>
    <define name="MAX_CMD_ROLL_ANGLE" value="25.0" unit="deg"/>
    <define name="MAX_CMD_YAW_RATE" value="100.0" unit="deg/s"/>
  </section>

  <section name="FWD_LIMITS" prefix="LIMITS_FWD_">
    <!-- Maximum limits of the vehicle in manual speed control mode. limits are in the control rf -->
    <define name="MAX_FWD_SPEED" value="15.0" unit="m/s"/>
    <define name="MIN_FWD_SPEED" value="-3.0" unit="m/s"/>
    <define name="MAX_LAT_SPEED" value="5.0" unit="m/s"/>
    <define name="MAX_VERT_SPEED" value="2.0" unit="m/s"/>
    <define name="MAX_FWD_ACC" value="6.0" unit="m/s^2"/>
    <define name="MAX_LAT_ACC" value="5.0" unit="m/s^2"/>
    <define name="MAX_VERT_ACC" value="5.0" unit="m/s^2"/>
  </section>


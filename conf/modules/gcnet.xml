<!DOCTYPE module SYSTEM "module.dtd">

<module name="gcnet">
  <doc>
    <description>
    This module is used to test the gcnet controller.
    Select the folder of the desired network in the define below,
    </description>
  </doc>

  <header>
    <file name="gcnet_main.h"/>
  </header>
  <init fun="gcnet_init()"/>
  <periodic fun="gcnet_run()" freq="512" />
  <!--datalink message="EXTERNAL_VISION" fun="external_vision_update(buf)"/-->

  <makefile>
    <file name="gcnet_main.c"/>
    <file name="nn_operations.c"/>
    <!-- Select the desired network -->
    <define value="free_final_velocity/nn_parameters.h" name="GNC_NET_NN_PARAM_H" type="string"/>
    <file    name="free_final_velocity/nn_parameters.c"/>
  </makefile>
</module>

 <aerodynamics>
  <axis name="YAW">
    <function name="rud_yaw" frame="BODY" unit="FT*LBS">
      <product>
        <property>fcs/rud_lag</property>
        <property>aero/qbar-psf</property>
        <value>47.9</value> <!--Conversion to pascals -->
        <value> 0.094 </value>
        <value>0.224808943</value> <!-- N to LBS -->
      </product>
    </function>
  </axis>

  <axis name="PITCH">
    <function name="elev_pitch" frame="BODY" unit="FT*LBS">
      <product>
        <property>fcs/elev_lag</property>
        <property>aero/qbar-psf</property>
        <value>47.9</value> <!--Conversion to pascals -->
        <value> -0.157 </value>
        <value>0.224808943</value> <!-- N to LBS -->
      </product>
    </function>
  </axis> 

  <axis name="ROLL">
    <function name="ail_roll" frame="BODY" unit="FT*LBS">
      <sum>
        <product>
          <property>fcs/ail_left_lag</property>
          <property>aero/qbar-psf</property>
          <value>47.9</value> <!--Conversion to pascals -->
          <value> 0.204 </value>
          <value> 10.0 </value>
          <value>0.224808943</value> <!-- N to LBS -->
        </product>

        <product>
          <property>fcs/ail_right_lag</property>
          <property>aero/qbar-psf</property>
          <value>47.9</value> <!--Conversion to pascals -->
          <value> 0.204 </value>
          <value> 10.0 </value>
          <value>0.224808943</value> <!-- N to LBS -->
        </product>
      </sum>
    </function>
  </axis>

  <axis name="LIFT">
    <function name="aero/force/Lift_alpha">
      <description>Lift due to alpha</description>
      <product>
          <property>aero/qbar-psf</property>
          <value>47.9</value> <!--Conversion to pascals -->
          <!--<property>metrics/Sw-sqft</property>-->
          <value>0.578</value>
          <sin><product><v>1.5708</v><property>fcs/wing_rotation</property></product></sin>
          <table>
            <independentVar lookup="row">aero/alpha-deg</independentVar>
            <tableData>
              -90   0.0
              -50  -0.80
              -20  -0.63
              -12  -0.74
              -8  -0.64
              0  0.07
              11  1.15
              14  1.2
              18  1.3
              45  0.8
              90   0.0
              135  0.8
              180  0.0
              225 -0.8
              270 0.0
            </tableData>
          </table>
          <value>0.224808943</value> <!-- N to LBS -->
          <value>2.0</value>
      </product>
    </function>

  </axis>


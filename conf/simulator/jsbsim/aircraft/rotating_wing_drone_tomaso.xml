    <mass_balance>
        <ixx unit="SLUG*FT2">0.03328</ixx>
        <iyy unit="SLUG*FT2">0.25055</iyy>
        <izz unit="SLUG*FT2">0.1737</izz>
        <ixy unit="SLUG*FT2">0.</ixy>
        <ixz unit="SLUG*FT2">0.</ixz>
        <iyz unit="SLUG*FT2">0.</iyz>
